<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>文件下载管理后台</title>

    <!-- Bootstrap Core CSS -->
    <link href="/assets/bootstrap/css/bootstrap.min.css" rel="stylesheet">

    <!-- MetisMenu CSS -->
    <link href="/assets/metismenu/css/metisMenu.min.css" rel="stylesheet">

    <!-- Custom Fonts -->
    <link href="/assets/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
  <%= stylesheet_link_tag    'application', media: 'all'%>

  <%= csrf_meta_tags %>
  <%= yield :headers %>

</head>
<body>
  <div id="wrapper">
    <!-- Navigation -->
    <nav class="navbar navbar-default navbar-static-top" role="navigation" style="margin-bottom: 0">
      <%= render 'partials/headbar' %>
      <%= render 'partials/sidebar' %>
    </nav>
    <div id="page-wrapper">
       <% unless notice.nil? %><div class="alert alert-success" role="alert"> <%= notice%></div><% end -%>
       <% unless alert.nil? %><div class="alert alert-danger" role="alert"> <%= alert%></div><% end -%>
<%= yield %>
    </div>

  </div>
  <script src="/assets/jquery/jquery.min.js"></script>

  <!-- Bootstrap Core JavaScript -->
  <script src="/assets/bootstrap/js/bootstrap.min.js"></script>

  <!-- Metis Menu Plugin JavaScript -->
  <script src="/assets/metismenu/js/metisMenu.min.js"></script>

  <%= javascript_include_tag 'application'%> 

  <%= yield :scripts %>
</body>
</html>
