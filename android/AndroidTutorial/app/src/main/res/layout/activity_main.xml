<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".android.MainActivity">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Interact Example"
            android:id="@+id/interact_example_link"
            />
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Fragment Examples"
            android:id="@+id/fragment_examples_link"
            />
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="List Examples"
            android:id="@+id/list_examples_link"
            />

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Recycler View Examples"
            android:id="@+id/recycler_view_examples_link"
            />
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Async Call Examples"
            android:id="@+id/async_call_examples_link"
            />
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Drawable Examples"
            android:id="@+id/drawable_examples_link"
            />
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Activity Life Cycle Example"
            android:id="@+id/act_life_cycle_example_link"
            />
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Serialization Examples"
            android:id="@+id/serialization_examples_link"
            />
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Data Process Examples"
            android:id="@+id/data_process_examples_link"
            />
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Menu Examples"
            android:id="@+id/menu_examples_link"
            />
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Customization Examples"
            android:id="@+id/customization_examples_link"/>
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Data Provider Examples"
            android:id="@+id/data_provider_examples_link"
            />
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Pager Examples"
            android:id="@+id/pager_examples_link"
            />
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Return Result Examples"
            android:id="@+id/return_result_example_link"
            />
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Search Examples"
            android:id="@+id/search_examples_link"
            />
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Service Examples"
            android:id="@+id/service_examples_link"
            />
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Broadcast Receiver Examples"
            android:id="@+id/broadcast_receiver_examples_link"
            />
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Notification Examples"
            android:id="@+id/notification_examples_link"
            />
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="WebView Examples"
            android:id="@+id/webview_examples_link"
            />
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Fix Screen Examples"
            android:id="@+id/fix_multi_screen_examples_link"
            />
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Animation Examples"
            android:id="@+id/animation_example_link"/>
    </LinearLayout>
</ScrollView>