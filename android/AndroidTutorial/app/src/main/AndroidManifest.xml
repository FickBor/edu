        <activity android:name="org.shenit.tutorial.android.drawable.SimpleDrawableExampleActivity" />
        <activity android:name="org.shenit.tutorial.android.drawable.RemoteDrawableExampleActivity" />
        <activity android:name="org.shenit.tutorial.android.AsyncTaskExampleActivity" />
        <activity android:name="org.shenit.tutorial.android.ExecutorExampleActivity" />
        <activity
            android:name="org.shenit.tutorial.android.MainActivity"
            android:label="@string/title_activity_main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="org.shenit.tutorial.android.InteractExampleActivity" />
        <activity android:name="org.shenit.tutorial.android.HelloWorldActivity" />
        <activity android:name="org.shenit.tutorial.android.drawable.RemoteDrawableListExampleActivity" />
        <activity android:name="org.shenit.tutorial.android.drawable.GlideExampleActivity" />
