        <activity android:name="org.shenit.tutorial.android.dataproc.InternalStorageExampleActivity" />
        <activity android:name="org.shenit.tutorial.android.dataproc.SharedPreferencesExampleActivity" />
        <activity android:name="org.shenit.tutorial.android.dataproc.ExternalStorageExampleActivity" />
        <activity android:name="org.shenit.tutorial.android.dataproc.SQLiteStorageExampleActivity" />
        <activity android:name="org.shenit.tutorial.android.AsyncTaskLoaderExampleActivity" />
        <activity android:name="org.shenit.tutorial.android.asynccall.AsyncCallExamplesActivity" />
